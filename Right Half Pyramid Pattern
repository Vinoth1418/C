#include <stdio.h>

int main() {
     // Number of rows for the reverse triangle

    // Loop for each row
    
int rows=5;
     for(int i=1;i<=rows;i++){
       for(int j=1;j<=i;j++){
             printf("* ") ;
     }
     printf("\n");
      
        
  
       
      
         
         
        }
   
   }
