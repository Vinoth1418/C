#include <stdio.h>

int main() {
    int rows=5; // Number of rows for the reverse triangle

    // Loop for each row
    for (int i = rows; i >= 1; i--) {
        // Print leading spaces
        for (int k = i; k < rows; k++) {
            printf(" ");
        }

        // Print stars
        for (int k = 1; k <= i; k++) {
            printf("* ");
        }

        // Move to the next line after printing stars
        printf("\n");
    }

    return 0;
}
