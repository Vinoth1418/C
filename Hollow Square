#include <stdio.h>

int main() {
     // Number of rows for the reverse triangle

    // Loop for each row
    

   for(int i=0;i<5;i++){
       for(int j=0;j<5;j++){
         if (i == 0 || j == 0 || i == 5 - 1 || j == 5- 1) {
                    printf("* ");}
                    else{
                    printf("  ");}       
     }
     printf("\n");
   }}
